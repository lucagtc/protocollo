<h1>Protocollo list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Anno</th>
            <th>Protocollonumero</th>
            <th>Dataregistrazione</th>
            <th>Tipo</th>
            <th>Intestazione</th>
            <th>Indirizzo</th>
            <th>Localita</th>
            <th>Datadocumento</th>
            <th>Oggetto</th>
            <th>Protocollodocumento</th>
            <th>Documento</th>
            <th>Categoria</th>
            <th>Classificazione</th>
            <th>Fascicolo</th>
            <th>Utente</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('protocollo_protocollo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.anno }}</td>
            <td>{{ entity.protocolloNumero }}</td>
            <td>{% if entity.dataRegistrazione %}{{ entity.dataRegistrazione|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.tipo }}</td>
            <td>{{ entity.intestazione }}</td>
            <td>{{ entity.indirizzo }}</td>
            <td>{{ entity.localita }}</td>
            <td>{% if entity.dataDocumento %}{{ entity.dataDocumento|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.oggetto }}</td>
            <td>{{ entity.protocolloDocumento }}</td>
            <td>{{ entity.documento }}</td>
            <td>{{ entity.categoria }}</td>
            <td>{{ entity.classificazione }}</td>
            <td>{{ entity.fascicolo }}</td>
            <td>{{ entity.utente }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('protocollo_protocollo_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('protocollo_protocollo_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('protocollo_protocollo_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
