<h1>Protocollo edit</h1>

<form action="{{ path('protocollo_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Salva</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('protocollo') }}">
            Torna alla lista
        </a>
    </li>
    <li>
        <form action="{{ path('protocollo_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Cancella</button>
        </form>
    </li>
</ul>
