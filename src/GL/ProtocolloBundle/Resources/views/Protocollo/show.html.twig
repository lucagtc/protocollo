<h1>Protocollo</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Anno</th>
            <td>{{ entity.anno }}</td>
        </tr>
        <tr>
            <th>Protocollo</th>
            <td>{{ entity.protocollo }}</td>
        </tr>
        <tr>
            <th>Ui</th>
            <td>{{ entity.UI }}</td>
        </tr>
        <tr>
            <th>Data</th>
            <td>{{ entity.data|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Tipo</th>
            <td>{{ entity.tipo }}</td>
        </tr>
        <tr>
            <th>Nome</th>
            <td>{{ entity.nome }}</td>
        </tr>
        <tr>
            <th>Indirizzo</th>
            <td>{{ entity.indirizzo }}</td>
        </tr>
        <tr>
            <th>Localita</th>
            <td>{{ entity.localita }}</td>
        </tr>
        <tr>
            <th>Oggetto</th>
            <td>{{ entity.oggetto }}</td>
        </tr>
        <tr>
            <th>Posizione</th>
            <td>{{ entity.posizione }}</td>
        </tr>
        <tr>
            <th>User_id</th>
            <td>{{ entity.userid }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updated_at</th>
            <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('protocollo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('protocollo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('protocollo_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
